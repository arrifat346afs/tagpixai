  "description": " AI powered metadata generator",
  "author": "AR Rifat",
  "scripts": {
    "dev": "npm-run-all --parallel dev:r dev:e",
    "dev:r": "vite",
    "dev:e": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64 --config electron-builder.json",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64 --config electron-builder.json",
    "postinstall": "electron-builder install-app-deps"
  },
